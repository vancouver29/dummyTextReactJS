}},{"generated":{"line":122,"column":35},"source":"components/DummyText/DummyText.js","original":{"line":69,"column":45}},{"generated":{"line":122,"column":41},"source":"components/DummyText/DummyText.js","original":{"line":53,"column":17}},{"generated":{"line":122,"column":47},"source":"components/DummyText/DummyText.js","original":{"line":53,"column":8}},{"generated":{"line":123,"column":0},"source":"components/DummyText/DummyText.js","original":{"line":73,"column":5}},{"name":"Component","generated":{"line":127,"column":2},"source":"components/DummyText/DummyText.js","original":{"line":3,"column":33}}],"sources":{"components/DummyText/DummyText.js":"import React, { Component } from \"react\";\n\nexport default class App extends Component {\n    constructor(props) {\n        super(props);\n        this.state = {\n            isLoading: true,\n            paragraphs: 3,\n            results: [],\n        };\n\n        this.handleChange = this.handleChange.bind(this);\n    }\n\n    componentDidMount() {\n        this.fetchText();\n    }\n\n    fetchText() {\n        // each time we make a request, isLoading turns true\n        this.setState({ isLoading: true });\n\n        const url =\n            \"https://baconipsum.com/api/?type=meat-and-filler&paras=\" +\n            this.state.paragraphs;\n        fetch(url)\n            .then((response) => response.json())\n            .then((data) => {\n                console.log(data);\n                this.setState({ results: data, isLoading: false });\n            });\n    }\n\n    // setSate is asynchronous function, so after setState finished\n    // the fetchText function can be called back\n    handleChange(ev) {\n        this.setState({ paragraphs: ev.target.value }, () => {\n            this.fetchText();\n        });\n    }\n\n    displayResults() {\n        if (this.state.isLoading) {\n            return <p > Loading... < /p>;\n        } else {\n            return this.state.results.map((paragraphText, index) => {\n                return <p key = { index } > { paragraphText } < /p>;\n            });\n        }\n    }\n\n    render() {\n        return ( <\n            div id = \"dummy-container\" >\n            <\n            div id = \"dummy-text-control\" >\n            <\n            h2 > Real time Options: < /h2> <\n            p > Paragraphs: < /p> <\n            p >\n            <\n            input min = \"1\"\n            type = \"number\"\n            value = { this.state.paragraphs }\n            onChange = { this.handleChange }\n            /> <\n            /p> <\n            /div> <\n            h1 > Dummy Text Generator < /h1> <\n            div id = \"dummy-text-result\" > { this.displayResults() } < /div> <\n            /div>\n        );\n    }\n}"},"lineCount":null}},"error":null,"hash":"0894b2e53278692f59eea36b12dcecb6","cacheData":{"env":{}}}